\n\nimport axios from \"axios\";\ninterface IResponse {\n  _id: string;\n  name: string;\n  message: string;\n}\nexport default {\n  name: \"app\",\n  data: () => {\n    return {\n      dados: [],\n      id: 0,\n      name: \"\",\n      message: \"\",\n      update: false,\n    };\n  },\n\n  methods: {\n    buscar() {\n      axios\n        .create({\n          baseURL: \"http://100.64.144.245:5000/\",\n          timeout: 10000,\n        })\n        .get(\"/mural\")\n        .then((response) => {\n          console.log(response);\n          this.dados = response.data.mural.map((element: IResponse) => {\n            return {\n              id: element._id,\n              name: element.name,\n              message: element.message,\n            };\n          });\n        })\n        .catch((error) => {\n          alert(error);\n        });\n    },\n\n    excluir(id) {\n      axios\n        .create({\n          baseURL: \"http://100.64.144.245:5000/\",\n          timeout: 10000,\n        })\n        .delete(\"/mural\", { data: { id: id } })\n        .then((response) => {\n          console.log(response);\n          this.buscar();\n        })\n        .catch((error) => {\n          alert(error);\n        });\n    },\n\n    enviar() {\n      if (!this.update) {\n        axios\n          .create({\n            baseURL: \"http://100.64.144.245:5000/\",\n            timeout: 10000,\n          })\n          .post(\"/mural\", { name: this.name, message: this.message })\n          .then((response) => {\n            console.log(response);\n            this.buscar();\n          })\n          .catch((error) => {\n            alert(error);\n          });\n      } else {\n        this.atualizar();\n      }\n    },\n\n    editar(dado) {\n      this.id = dado.id;\n      this.name = dado.name;\n      this.message = dado.message;\n      this.update = true;\n    },\n\n    atualizar() {\n      axios\n        .create({\n          baseURL: \"http://100.64.144.245:5000/\",\n          timeout: 10000,\n        })\n        .put(\"/mural\", { id: this.id, message: this.message })\n        .then((response) => {\n          console.log(response);\n          this.buscar();\n          this.update = false;\n          (this.name = \"\"), (this.message = \"\");\n        })\n\n        .catch((error) => {\n          alert(error);\n        });\n    },\n  },\n  created() {\n    this.buscar();\n  },\n};\n"],"sourceRoot":""}]}